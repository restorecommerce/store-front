{
  "name": "store-front",
  "version": "0.0.1",
  "description": "Customer facing UI for Restorecommerce shops",
  "author": "n-fuse GmbH",
  "repository": {
    "type": "git",
    "url": "https://github.com/restorecommerce/store-front.git"
  },
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
    "test": "ng test",
    "start": "npm-run-all generate serve",
    "generate": "gql-gen -c gql-codegen/gql-codegen-dev.yml",
    "generate:prod": "gql-gen -c gql-codegen/gql-codegen-prod.yml",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "assets": "rm -rf src/assets/products_responsive && ipp",
    "build": "run-s assets build:browser build:ssr",
    "serve:browser": "ng serve",
    "build:browser": "ng build",
    "dev:ssr": "ng run store-front:serve-ssr",
    "serve:ssr": "node dist/store-front/server/main.js",
    "build:ssr": "ng build --prod && ng run store-front:server:production",
    "prerender": "ng run store-front:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.2.3",
